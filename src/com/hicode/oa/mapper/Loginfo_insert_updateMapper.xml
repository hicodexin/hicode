<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hicode.oa.dao.Loginfo_insert_updateDAO">

	<sql id="all_column">
		loginup_id,userName,creatTime,log_ip,xiangQing
	</sql>

	<insert id="do_insertLogInfo" parameterType="Loginfo_insert_update"
		keyProperty="loginup_id">
		insert into Loginfo_insert_update
		(userName,creatTime,log_ip,xiangQing)
		values
		(#{userName},#{creatTime},#{log_ip},#{xiangQing});

	</insert>
</mapper>